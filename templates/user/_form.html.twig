<div class="container mt-3">
{{ form_start(form) }}
    <div class="row">
        <div class="col-md-3">{{form_row(form.nom)}}</div>
        <div class="col-md-3">{{form_row(form.prenom)}}</div>
        <div class="col-md-3">{{form_row(form.secteur)}}</div>
        <div class="col-md-3">{{form_row(form.roleMaker)}}</div>

    </div>
    <div class="row">
        <div class="col-md-4">{{form_row(form.email)}}</div>
        <div class="col-md-4">{{form_row(form.typeContrat) }}</div>
        <div class="col-md-4">{{form_row(form.dateSortie)}}</div>
    </div>
    <div class="row">
        <div class="col-md-8">{{form_row(form.password) }}</div>
    </div>

    <button class="btn btn-success">{{ button_label|default('Cr√©er') }}</button>
{{ form_end(form) }}
</div>
